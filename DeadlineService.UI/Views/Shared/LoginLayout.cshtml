@model ResponseModel<LoginDTO>;
<!DOCTYPE html>
<html lang="en">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/css/styles.min.css" asp-append-version="true" />
</head>
<body>
    <div>
        @RenderBody()
    </div>
    
